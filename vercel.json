{
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/redirect",
      "has": [
        { "type": "query", "key": "lp" }
      ],
      "destination": "/:lp",
      "permanent": false
    },
    {
      "source": "/redirect",
      "has": [
        { "type": "query", "key": "page" }
      ],
      "destination": "/:page",
      "permanent": false
    },
    {
      "source": "/",
      "has": [
        { "type": "query", "key": "lp" }
      ],
      "destination": "/:lp",
      "permanent": false
    },
    {
      "source": "/",
      "has": [
        { "type": "query", "key": "page" }
      ],
      "destination": "/:page",
      "permanent": false
    },
    {
      "source": "/redirect",
      "destination": "/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/:basePath*%7Bignore%7D",
      "destination": "/:basePath*"
    },
    {
      "source": "/:page((?!%7Bignore%7D).*)",
      "destination": "/:page"
    },
    {
      "source": "/:path*",
      "destination": "/"
    }
  ]
}
